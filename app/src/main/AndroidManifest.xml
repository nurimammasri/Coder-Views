<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projectview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivitySpinner" />
        <activity android:name=".ActivityProgressBar" />
        <activity android:name=".ActivityRadioButton" />
        <activity android:name=".ActivitySwitch" />
        <activity android:name=".ActivityToggleButton" />
        <activity android:name=".ActivityCheckBox" />
        <activity android:name=".ActivityImageButton" />
        <activity android:name=".ActivityEditText" />
        <activity android:name=".ActivityImageView" />
        <activity android:name=".ActivityTextView" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>